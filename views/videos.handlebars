<div class="container-fluid data-interval = 2000" id = 8>
   <div>
        <h1 class="d-flex justify-content-center"> Some Of Your Most Watched</h1>
        <h1 class="d-flex justify-content-center"> Videos Include</h1>
    </div>

  <div data-bs-spy="scroll" data-bs-target="#list-example" data-bs-offset="0" class="scrollspy-example" tabindex="0">
    {{#each links}}
    <div class="embed-responsive embed-responsive-16by9">
      <iframe class="embed-responsive-item"
          src="https://www.youtube.com/embed/{{this}}">
      </iframe>
    </div>
    {{/each}}
  </div>
  <div class = "row">
        <div class="col-lg-4"></div>
        <div class="col-lg-4 text-center">
            <a href="/topvideo"><button type = "button" class="btn btn-lg btn-block" id = "cssBtn" >See Most Played Video</button></a>
        </div>
        <div class="col-lg-4"></div>
    </div>
</div>



